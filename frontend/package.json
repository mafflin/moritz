{
  "name": "moritz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "actioncable": "^5.2.6",
    "axios": "^0.22",
    "core-js": "^3.18",
    "humps": "^2.0",
    "mapbox-gl": "^2.5",
    "vue": "^2.6",
    "vue-router": "^3.5",
    "vuetify": "^2.5.9",
    "vuex": "^3.6",
    "vuex-router-sync": "^5.0"
  },
  "devDependencies": {
    "@mdi/font": "^6.2",
    "@vue/cli-plugin-babel": "^4.5",
    "@vue/cli-plugin-eslint": "^4.5",
    "@vue/cli-service": "^4.5",
    "@vue/eslint-config-airbnb": "^5.3",
    "babel-eslint": "^10.1",
    "deepmerge": "^4.2",
    "eslint": "^7.32",
    "eslint-plugin-prettier": "^4.0",
    "eslint-plugin-vue": "^7.19",
    "fibers": "^5.0",
    "sass": "^1.42",
    "sass-loader": "^10",
    "vue-template-compiler": "^2.6",
    "vuetify-loader": "^1.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "import/extensions": "off",
      "import/no-cycle": "off",
      "import/no-extraneous-dependencies": "off",
      "no-console": "off",
      "no-debugger": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
